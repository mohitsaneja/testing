<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd">
   <bean id="ans1" class="Dependent_Object.Answere">
   <constructor-arg value="1"></constructor-arg>
   <constructor-arg value="Java is Platform Independent"></constructor-arg>
   <constructor-arg value="John"></constructor-arg> 
   </bean>
   <bean id="ans2" class="Dependent_Object.Answere">
   <constructor-arg value="2"></constructor-arg>
   <constructor-arg value="Java is Robust"></constructor-arg>
   <constructor-arg value="Ravi"></constructor-arg> 
   </bean>
   <bean id = "question" class="Dependent_Object.Question">
   <constructor-arg value="111"></constructor-arg>
   <constructor-arg value="What is Java?"></constructor-arg>
   <constructor-arg>
   <list>
   <ref bean="ans1" />
   <ref bean="ans2"></ref>
   </list>
   </constructor-arg>
   </bean> 
</beans>